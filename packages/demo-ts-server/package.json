{
  "name": "@joenoon/demo-ts-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "db:migrate:dev": "test -f \"$OBEVO_HOME/bin/deploy.sh\" && \"$OBEVO_HOME/bin/deploy.sh\" DEPLOY -sourcePath deltas/ -env dev",
    "build:source": "tsc",
    "build:graphql": "ts-node src/genSchema.ts",
    "build:assets": "cp ./src/schema/ro.schema.json ./build/schema/",
    "clean": "rimraf ./build",
    "graphql:playground": "graphql playground --port 4000",
    "dev:start": "yarn run build:graphql && ts-node src/index.ts",
    "dev": "nodemon --watch src --ext js,ts,json --ignore 'src/schema/ro.schema.d.ts' --ignore 'src/schema/ro.schema.json' --verbose --exec yarn -- dev:start"
  },
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/cookie-session": "^2.0.34",
    "@types/core-js": "^2.5.0",
    "@types/express": "^4.16.0",
    "@types/history": "^4.7.0",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/node": "^10.3.4",
    "@types/sequelize": "^4.27.24",
    "@types/uuid": "^3.4.3",
    "apollo-server-express": "^1.3.6",
    "base64url": "^3.0.0",
    "camelcase": "^5.0.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.0.0-beta.10",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "graphql-cli": "^2.16.3",
    "graphql-code-generator": "^0.10.5",
    "graphql-codegen-typescript-template": "^0.10.5",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.2",
    "typescript": "^3.1.1"
  }
}
